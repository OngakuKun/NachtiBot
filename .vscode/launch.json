		{
			"name": "Run",
			"type": "cppdbg",
			"request": "launch",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"program": "${workspaceFolder}/bin/RisutoMaster",
			"MIMode": "gdb",
			"miDebuggerPath": "/usr/bin/gdb",
			"preLaunchTask": "build release",
		}
	]
